{
    "name": "OpenStudio Python DevContainer",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "customizations": {
        "vscode": {
            "settings": {
                "python.defaultInterpreterPath": "/workspaces/SimParc/.venv/bin/python3"
            },
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
				"ms-toolsai.jupyter",
				"ms-toolsai.datawrangler",
				"qwtel.sqlite-viewer"
            ]
        }
    },
    "postCreateCommand": "uv sync"
}
